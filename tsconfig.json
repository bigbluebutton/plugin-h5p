{
    "compilerOptions": {
        "outDir": "./dist-tsc/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node"
    },
    "include": ["src/*"],
    "paths": {
        "*": ["node_modules"]
    },
    "exclude": ["node_modules", "lib/h5p-standalone", "lib/h5p-standalone-from-tunapanda"]
}
